# Lean API Configuration
# Copy this file to .env and fill in your actual credentials

# Lean Client ID (App Token)
LEAN_CLIENT_ID=your_client_id_here

# Lean Client Secret (Keep this SECRET - never commit to Git!)
LEAN_CLIENT_SECRET=your_client_secret_here

# Lean API URLs (Sandbox or Production)
LEAN_AUTH_URL=https://auth.sandbox.sa.leantech.me/oauth2/token
LEAN_API_URL=https://sandbox.sa.leantech.me

# Server Configuration
PORT=8000
